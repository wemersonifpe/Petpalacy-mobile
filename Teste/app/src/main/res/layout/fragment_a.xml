<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/conainerA"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <!--TextView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:gravity="center_horizontal|center_vertical"
        android:text="@string/text_fragment_a" /-->

    <Button
        android:id="@+id/button_clinicas"
        android:layout_width="128dp"
        android:layout_height="59dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="68dp"
        android:layout_marginTop="180dp"
        android:text="Clinicas Veterinarias"
        android:visibility="visible"/>

    <Button
        android:id="@+id/button_petshops"
        android:layout_width="wrap_content"
        android:layout_height="59dp"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/button_clinicas"
        android:layout_marginEnd="70dp"
        android:text="PetShops"
        android:visibility="visible"/>

    <FrameLayout
        android:id="@+id/container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="invisible"/>

</RelativeLayout>